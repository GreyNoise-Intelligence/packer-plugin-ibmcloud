# Colors
red="\[\033[1;31m\]"  
red=$'\e[1;31m'
green=$'\e[1;32m'
blue=$'\e[1;34m'
purple=$'\e[1;35m'
cyan=$'\e[1;36m'
white=$'\e[0m'
yellow=$'\e[1;33m'

go_tar="go1.14.1.linux-amd64.tar.gz"
go_url="https://dl.google.com/go/$go_tar"

packer_zip="packer_1.6.1_linux_amd64.zip"
packer_url="https://releases.hashicorp.com/packer/1.6.1/$packer_zip"
